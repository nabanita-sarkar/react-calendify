{
  "name": "react-calendify",
  "version": "0.1.1",
  "description": "React calendar to show events",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nabanita-sarkar/react-events-calendar"
  },
  "author": {
    "name": "Nabanita Sarkar"
  },
  "publishConfig": {
    "name": "react-calendify",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/react-calendify.umd.js",
  "module": "dist/react-calendify.es.js",
  "exports": {
    ".": {
      "import": "dist/react-calendify.es.js",
      "require": "dist/react-calendify.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "keywords": [
    "react",
    "calendar",
    "events",
    "schedule"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "dependencies": {
    "dayjs": "^1.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "sass": "^1.51.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-checker": "^0.3.4"
  }
}
